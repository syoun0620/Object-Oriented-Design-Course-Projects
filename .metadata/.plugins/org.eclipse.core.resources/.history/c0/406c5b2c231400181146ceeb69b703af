	class SomeClass {


		   void divide( int num, int den) throws Exception {


			   if( den == 0) {
				    throw new Exception("DivideByZeroException");
				}


		       System.out.println( “” + (num / den) );


		   }


		   void fun() {


			   try {
				    divide( 4, 2);
				} catch (Exception e) { }


		   } 


		}
}
